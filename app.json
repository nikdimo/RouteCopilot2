{
  "expo": {
    "name": "WisePlan",
    "slug": "routecopilot2",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.wiseplan.app",
      "buildNumber": "19",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.wiseplan.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBtZzYqQANPbwQBs0l_8lJn4DuQ-YnD-XI"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "baseUrl": "/app/"
    },
    "experiments": {
      "baseUrl": "/app/"
    },
    "plugins": [
      "expo-web-browser",
      "expo-location",
      "expo-secure-store",
      [
        "react-native-maps",
        {}
      ],
      "./plugins/withAndroidConfigChanges.js"
    ],
    "scheme": "wiseplan",
    "extra": {
      "eas": {
        "projectId": "716beeea-e5b0-4871-89be-16a216191644"
      }
    },
    "owner": "nikdim"
  }
}