{
  "expo": {
    "name": "WisePlan",
    "slug": "wiseplan",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.wiseplan.app",
      "buildNumber": "4",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.wiseplan.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "baseUrl": "/app/"
    },
    "experiments": {
      "baseUrl": "/app/"
    },
    "plugins": [
      "expo-web-browser",
      "expo-location",
      "expo-secure-store"
    ],
    "scheme": "wiseplan",
    "extra": {
      "eas": {
        "projectId": "4d926b50-153f-408d-8f89-a4c79a123524"
      }
    }
  }
}
